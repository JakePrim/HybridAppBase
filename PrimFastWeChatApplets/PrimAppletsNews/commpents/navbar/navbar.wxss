.nav-wrap {
  position: fixed;
  width: 100%;
  top: 0;
  /* background: #3299dc; */
  color: #000;
  z-index: 9999999;
}
/* 标题要居中 */
.nav-title {
  position: absolute;
  /* text-align: center; */
  /* max-width: 400rpx; */
  overflow: hidden;
  /* text-overflow: ellipsis; */
  /* white-space: nowrap; */
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin-left: 100rpx;
